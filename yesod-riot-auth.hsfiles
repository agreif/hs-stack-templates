                <a onclick="{changeLanguageEn}">EN
        <li>
          <a onclick="{editMyProfile}">
            {context.data.translation.msgGlobalMyProfile}
        <li>
          <a href=@{AuthR LogoutR}>
            <div>
              {context.data.translation.msgGlobalLogout}
