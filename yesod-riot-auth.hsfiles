{-# START_FILE templates/login-layout-wrapper.hamlet #-}
<!doctype html>
<html class="no-js" lang="en">
  <head>
    <meta charset="UTF-8">
    <title>#{pageTitle pc}

    <link href="@{StaticR css_uikit_min_css}" rel="stylesheet" type="text/css">
    <script src="@{StaticR js_uikit_min_js}" type="text/javascript">
    <script src="@{StaticR js_uikit_icons_min_js}" type="text/javascript">

    ^{pageHead pc}

  <body>
    ^{pageBody pc}

{-# START_FILE templates/login_form.hamlet #-}
<div .uk-margin-xlarge-top .uk-flex .uk-flex-center>
  <h1>
    My-Project
<div .uk-margin-top .uk-flex .uk-flex-center>
  <form #login-form .uk-form-horizontal method=post action=@{loginRoute}>
    $maybe token <- maybeToken
      <input type=hidden name=#{defaultCsrfParamName} value=#{token}>
      <div .uk-margin>
        <label .uk-form-label for="form-horizontal-text">
          Login
        <div .uk-form-controls>
          <input .uk-input type="text" name="username" placeholder="Anmeldename" required autofocus>
      <div .uk-margin>
        <label .uk-form-label for="form-horizontal-text">
          Passwort
        <div .uk-form-controls>
          <input .uk-input type="password" name="password" placeholder="Passwort" required>
      <div .uk-margin>
        <div .uk-form-controls>
          <button .uk-button .uk-button-primary href="@{AuthR LoginR}">Login

{-# START_FILE templates/riot-layout-wrapper.hamlet #-}
<!doctype html>
<html>
  <head>
    <meta charset="UTF-8">
    <title>#{pageTitle pc}

    <script src="@{StaticR js_jquery_3_3_1_min_js}" type="text/javascript">

    <link href="@{StaticR css_uikit_min_css}" rel="stylesheet" type="text/css">
    <script src="@{StaticR js_uikit_min_js}" type="text/javascript">
    <script src="@{StaticR js_uikit_icons_min_js}" type="text/javascript">


    <link href="@{StaticR css_tingle_min_css}" rel="stylesheet" type="text/css">
    <script src="@{StaticR js_tingle_min_js}" type="text/javascript">

    $if isDev
      <script src="@{StaticR js_riot_compiler_min_js}" type="text/javascript">
      <script data-src="@{MyprojectR RiotBodyTagR}" type="riot">
      <script data-src="@{MyprojectR RiotNavTagR}" type="riot">
      <script data-src="@{MyprojectR RiotPaginationTagR}" type="riot">
      <script data-src="@{MyprojectR RiotRawTagR}" type="riot">
      <script data-src="@{MyprojectR RiotHomePageTagR}" type="riot">
      <script data-src="@{MyprojectR RiotAdminPageTagR}" type="riot">
      <script data-src="@{MyprojectR RiotDemoaListPageTagR}" type="riot">
      <script data-src="@{MyprojectR RiotDemobListPageTagR}" type="riot">
      <script data-src="@{MyprojectR RiotDemobDetailPageTagR}" type="riot">
    $else
      <script src="@{StaticR js_riot_min_js}" type="text/javascript">

    ^{pageHead pc}

  <body>
    ^{pageBody pc}

{-# START_FILE templates/riot/admin_page_tag.hamlet #-}
<admin-page-tag>
  <div if="{context.data.pages.admin}">
    <h1>
      {context.data.translation.msgGlobalAdmin}
    <ul uk-tab>
      <li>
        <a href="#">
          {context.data.translation.msgUserUsers}
      <li>
        <a href="#">
          {context.data.translation.msgConfigConfigurations}
      <li>
        <a href="#">
          {context.data.translation.msgTestmailTestMail}
      <li>
        <a href="#">
          {context.data.translation.msgGlobalInfo}
    <ul .uk-switcher .uk-margin>
      <li>
        <!-- user list -->
        <table .uk-table .uk-table-small .uk-table-divider .uk-table-hover .uk-background-muted>
          <thead>
            <tr>
              <th>
                {context.data.translation.msgUserIdent}
              <th>
                {context.data.translation.msgUserEmail}
              <th>
                {context.data.translation.msgUserIsAdmin}
              <th .uk-align-right>
                <button .uk-background-primary .uk-light .uk-padding-small .uk-background-primary .uk-light .uk-padding-small .modal-form-link .uk-button .uk-button-link title="{context.data.translation.msgUserAddUser}" uk-tooltip onclick="{addUser}">
                  <span uk-icon="icon: plus">
          <tbody>
            <tr each="{user in context.data.pages.admin.users}">
              <td .uk-table-link>
                <a onclick="{(e)=>editUser(e,user)}" .uk-link-reset>
                  {user.entity.ident}
              <td .uk-table-link>
                <a onclick="{(e)=>editUser(e,user)}" .uk-link-reset>
                  {user.entity.email}
              <td .uk-table-link>
                <a onclick="{(e)=>editUser(e,user)}" .uk-link-reset>
                  <span uk-icon="icon: check" if="{user.entity.isAdmin}">
